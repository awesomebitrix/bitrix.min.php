    "debugType": "portable"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        },
        "Peachpie.App": "0.4.0-*"
      }
    }
  }
}
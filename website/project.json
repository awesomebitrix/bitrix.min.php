{
  "name": "website",
  "version": "1.0.0",
  "dependencies": {},
  "tools": {
    "Peachpie.Compiler.Tools": "0.4.0-*"
  },
  "buildOptions": {
    "compilerName": "php",
    "compile": [
      "**\\index.php",
      "**\\bitrix\\header.php",
      "**\\bitrix\\footer.php",
      "**\\bitrix\\php_interface\\init.php",
      "**\\bitrix\\modules\\main\\include.php",
      "**\\bitrix\\modules\\hello.world\\include.php",
      "**\\bitrix\\templates\\.default\\header.php",
      "**\\bitrix\\templates\\.default\\footer.php",
